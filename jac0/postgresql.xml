<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category>Network:Management Productivity: Tools:</Category>
  <Name>PostgreSQL</Name>
  <Description>
    PostgreSQL server for Docker with unRAID
  </Description>
  <Support>https://www.jcor.me</Support>
  <Registry>https://registry.hub.docker.com/u/sameersbn/postgresql/</Registry>
  <GitHub>https://github.com/sameersbn/docker-postgresql</GitHub>
  <Repository>sameersbn/postgresql</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>DB_NAME</Name>
      <Value>tower</Value>
    </Variable>
    <Variable>
      <Name>DB_USER</Name>
      <Value>tower</Value>
    </Variable>
    <Variable>
      <Name>DB_PASS</Name>
      <Value>tower</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5432</HostPort>
        <ContainerPort>5432</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/apps/postgresql</HostDir>
      <ContainerDir>/var/lib/postgresql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner>https://wiki.postgresql.org/images/1/1a/PostgreSQL.Banner.blue.468x60.gif</Banner>
  <Icon>http://www.iconattitude.com/icons/open_icon_library/apps/png/256/postgresql.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
